                    <GradientStop Color="#FFA3AEB9" Offset="0"/>
                    <GradientStop Color="#FF8399A9" Offset="0.375"/>
                    <GradientStop Color="#FF718597" Offset="0.375"/>
                    <GradientStop Color="#FF617584" Offset="1"/>
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:MultiValueComboBox">
                    <Grid>
                        <Grid.Resources>
                            <Style x:Key="CheckBoxListBoxItemStyle" TargetType="ListBoxItem">
                                <Setter Property="Foreground" Value="#FF000000" />
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="ListBoxItem">
                                            <Grid x:Name="RootElement">
                                                <CheckBox x:Name="PART_check" ClickMode="Press" Content="{Binding Path=DisplayField}" IsChecked="{Binding Path=IsSelected, Mode=TwoWay}" />
                                            </Grid>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </Grid.Resources>


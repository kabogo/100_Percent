                                <ListBox x:Name="comboListBox" SelectionMode="Multiple" ItemsSource="{TemplateBinding ItemsSource}" ItemContainerStyle="{StaticResource CheckBoxListBoxItemStyle}"  HorizontalAlignment="Stretch"/>
                                <!--</ScrollViewer>-->

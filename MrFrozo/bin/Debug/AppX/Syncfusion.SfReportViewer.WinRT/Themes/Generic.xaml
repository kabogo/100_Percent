                                                        <Grid.RowDefinitions>
                                                            <RowDefinition/>
                                                            <RowDefinition/>
                                                        </Grid.RowDefinitions>
                                                        <TextBox Name="Username" BorderBrush="Black" BorderThickness="1" Height="30" Grid.Row="0" Width="300" />
                                                        <PasswordBox Name="Password" BorderBrush="Black" BorderThickness="1" Height="30" Grid.Row="1" Width="300" />
                                                    </Grid>

                                                </Grid>
                                            </StackPanel>

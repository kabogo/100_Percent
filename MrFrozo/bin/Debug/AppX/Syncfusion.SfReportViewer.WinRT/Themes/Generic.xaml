                                                </StackPanel>
                                            </Grid>


                                            <StackPanel Margin="59,10,82,-24" Grid.Row="1" HorizontalAlignment="Right" Orientation="Horizontal" Grid.RowSpan="2">
                                                <Button x:Name="CredNext" Content="Next" Margin="10,0,10,0" Width="130"/>
                                                <Button x:Name="CredViewreport" Content="View Report" Margin="10,0,10,0" Width="130" IsEnabled="False"/>
                                            </StackPanel>
                                        </Grid>
                                    </ContentControl>
                                </Grid>
                            </ContentControl>


                            <Grid x:Name="NavigationPanelGrid" Grid.Row="4">
                                <Grid Background="Black">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid x:Name="NavigationBar"  Grid.Column="0" HorizontalAlignment="Left" Width="450" >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Button Style="{StaticResource FirstAppBarButtonStyle}"  VerticalAlignment="Center" Grid.Column="0" x:Name="First" IsEnabled="False"/>
                                        <Button Style="{StaticResource PreviousAppBarButtonStyle}"  VerticalAlignment="Center" x:Name="Previous" Grid.Column="1" IsEnabled="False"/>
                                        <Button Style="{StaticResource NextAppBarButtonStyle}"  VerticalAlignment="Center" Grid.Column="2" x:Name="Next" IsEnabled="False"/>
                                        <Button Style="{StaticResource LastAppBarButtonStyle}"  VerticalAlignment="Center" Grid.Column="3" x:Name="Last" IsEnabled="False"/>
                                        <Button Style="{StaticResource BackAppBarButtonStyle}"  VerticalAlignment="Center" x:Name="Back" Grid.Column="4" IsEnabled="True" Visibility="Collapsed"/>
                                        <Button Style="{StaticResource RefreshAppBarButtonStyle}"  VerticalAlignment="Center" Grid.Column="5" x:Name="Refresh" IsEnabled="False"/>
                                        <Button Style="{StaticResource DocumentMapAppBarButtonStyle}"  VerticalAlignment="Center" Grid.Column="6" x:Name="DocumentMap" IsEnabled="True" Visibility="Collapsed"/>
                                    </Grid>
                                    <Grid Grid.Column="1" HorizontalAlignment="Right" Width="300">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Button x:Name="Print" Style="{StaticResource PrintSelectionAppBarButtonStyle}"  Grid.Column="0"  VerticalAlignment="Center" Visibility="Visible" IsEnabled="False"></Button>
                                        <Button x:Name="Parameter" Style="{StaticResource ParameterAppBarButtonStyle}" IsEnabled="False"  Grid.Column="1"  VerticalAlignment="Center" BorderThickness="0"></Button>
                                        <Button x:Name="ExportButton" Style="{StaticResource ReportExportAppBarButtonStyle}" Grid.Column="2" VerticalAlignment="Center" BorderThickness="0" IsEnabled="False"></Button>
                                    </Grid>

                                </Grid>
                            </Grid>
                            
